@model Staff

<div class="container">
	<div class="row">

		<form method="post" asp-controller="Create" asp-action="Update">
			<div class="col-lg-6 offset-3">
				
				

				<div class="form-group">
					<lable>STAFF NAME : <input type="text" asp-for="@Model.Name" class="form-control" /></lable>
				</div>

				<br />

				<div class="form-group">
					<lable>STAFF WORK : <input type="text" asp-for="@Model.Work" class="form-control" /></lable>
